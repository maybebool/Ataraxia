                    <engine:Button text="Default Settings" name="DefaultSettingsButton" class="unity-button-custom" style="background-image: resource(&apos;Images/ButtonBackground&apos;); -unity-background-scale-mode: scale-and-crop; background-color: rgba(255, 255, 255, 0); margin-top: 10px; margin-right: 0; margin-bottom: 10px; margin-left: 0; color: rgb(233, 233, 233); border-top-width: 0; border-right-width: 0; border-bottom-width: 0; border-left-width: 0;" />
